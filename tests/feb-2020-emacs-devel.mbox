@@ -997,12 +997,13 @@ xg_frame_set_char_size (struct frame *f, int width, =
int height)
     }
   else
     {
+      GdkWindow *gwin =3D gtk_widget_get_window (FRAME_GTK_OUTER_WIDGET (=
f));
+
       frame_size_history_add
 	(f, Qxg_frame_set_char_size_3, width, height,
 	 list2i (totalwidth, totalheight));

-      gtk_window_resize (GTK_WINDOW (FRAME_GTK_OUTER_WIDGET (f)),
-			 totalwidth, totalheight);
+      gdk_window_resize (gwin, (gint)totalwidth, (gint)totalheight);
       fullscreen =3D Qnil;
