   unblock_input ();

+  if (FRAME_PARENT_FRAME (f) && x_resize_child_frame_special)
+    adjust_frame_size (f, 1000, 1000, 0, true, Qx_create_frame_2);
+
   adjust_frame_size (f, FRAME_TEXT_WIDTH (f), FRAME_TEXT_HEIGHT (f),
